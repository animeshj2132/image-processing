{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "import/extensions": "off",
      "no-underscore-dangle": "off",
      "prettier/prettier": "error"
    },
    "plugins": ["prettier"]
  }
  